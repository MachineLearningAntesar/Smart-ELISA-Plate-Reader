<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="73dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/resultTitleTView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageView
        android:id="@+id/orgDialogImage"
        android:layout_width="240dp"
        android:layout_height="180dp"
        android:adjustViewBounds="true"
        android:scaleType="centerInside"
        android:src="@drawable/tb_launcher"
        tools:layout_conversion_absoluteHeight="256dp"
        tools:layout_conversion_absoluteWidth="250dp"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="256dp"
        android:layout_marginLeft="44dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/resultTitleTView"
        app:layout_constraintVertical_bias="0.911" />

    <ImageView
        android:id="@+id/resultDialogImage"
        android:layout_width="240dp"
        android:layout_height="180dp"
        android:adjustViewBounds="true"
        android:scaleType="centerInside"
        android:src="@drawable/tb_launcher"
        tools:layout_conversion_absoluteHeight="218dp"
        tools:layout_conversion_absoluteWidth="152dp"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="256dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/orgDialogImage"
        android:layout_marginLeft="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/resultTitleTView"
        app:layout_constraintHorizontal_bias="0.23"
        app:layout_constraintVertical_bias="0.911" />

</android.support.constraint.ConstraintLayout>
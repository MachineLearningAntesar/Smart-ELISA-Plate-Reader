<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/tvIdProf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Professional Id"
        android:textSize="15dp"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@+id/etIdProf" />

    <EditText
        android:id="@+id/etIdProf"
        android:layout_width="180dp"
        android:layout_height="42dp"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="33dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="24dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintHorizontal_bias="0.767"
        app:layout_constraintLeft_toRightOf="@+id/tvIdProf"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include layout="@layout/divider"
        android:id="@+id/include"
        android:layout_height="2dp"
        android:layout_width="368dp"
        tools:layout_editor_absoluteX="8dp"
        app:layout_constraintTop_toBottomOf="@+id/etIdProf"/>


    <TextView
        android:id="@+id/tvIdPatient"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Patient Id"
        android:textSize="15dp"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@+id/etIdPatient" />

    <EditText
        android:id="@+id/etIdPatient"
        android:layout_width="180dp"
        android:layout_height="45dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/etId"
        android:layout_marginLeft="33dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintHorizontal_bias="0.543"
        app:layout_constraintLeft_toRightOf="@+id/tvIdPatient"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etIdProf" />

    <include layout="@layout/divider"
        android:id="@+id/include"
        android:layout_height="2dp"
        android:layout_width="368dp"
        tools:layout_editor_absoluteX="8dp"
        app:layout_constraintTop_toBottomOf="@+id/etIdPatient"/>

    <TextView
        android:id="@+id/tvPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Password"
        android:textSize="15dp"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@+id/etPassword" />

    <EditText
        android:id="@+id/etPassword"
        android:layout_width="180dp"
        android:layout_height="45dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/etAge"
        android:layout_marginLeft="33dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="textPostalAddress"
        app:layout_constraintHorizontal_bias="0.543"
        app:layout_constraintLeft_toRightOf="@+id/tvPassword"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etIdPatient" />

    <include
        layout="@layout/divider"
        android:layout_width="368dp"
        android:layout_height="2dp"
        app:layout_constraintTop_toBottomOf="@+id/etPassword"
        tools:layout_editor_absoluteX="8dp" />

</android.support.constraint.ConstraintLayout>

